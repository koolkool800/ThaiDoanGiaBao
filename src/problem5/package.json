        "express": "^4.21.2",
        "mysql2": "^3.12.0",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.5",
        "globals": "^15.14.0",
